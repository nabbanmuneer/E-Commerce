<!-- Body BEGIN -->
<body class="ecommerce">
    <!-- BEGIN STYLE CUSTOMIZER -->
    
    <!-- Header END -->
    
    <div class="main" style="min-height:100vh;" >
      <div class="container">
        <ul class="breadcrumb">
            <li><a href="/">Home</a></li>
            <li class="active">order List</li>
        </ul>
        
          <!-- END SIDEBAR -->

          <!-- BEGIN CONTENT -->
          <div class="col-md-12 col-sm-12">
            <h1>Shopping ordered items</h1>
            <div class="goods-page">
              <div class="goods-data clearfix">
                <div class="table-wrapper-responsive">
                  <table summary="" class="">
                    <tr>
                      <th class="goods-page-quantity">Order_id</th>
                    <th class="goods-page-quantity">Product Details</th>
                    <th class="goods-page-quantity">Delivary Address</th>
                    <th class="goods-page-quantity"> </th>
                    <th class="goods-page-quantity">Discount</th>
                    <th class="goods-page-quantity"> </th>
                    <th class="goods-page-quantity">total payment</th>
                    <th class="goods-page-quantity"> </th>
                    <th class="goods-page-price">Payment Method</th>
                    <th class="goods-page-total" colspan="2">delivery status</th>
                  </tr>
                  <% for(orders of order){%>
                    <div>
                      <tr>
                        <td>
                      <%=orders._id%>
                        </td>
                        <td>
                    <%for(data of orders.product){%>
                      <label>poduct Name :</label>
                      <%=data.product_Name%><br>
                      <label>poduct Size :</label>
                      <%=data.size%>
                      <label>poduct Quntity :</label>
                      <%=data.quantity%>
                    <br>

                    <%}%>
                        </td>
                        <td>
                  </div>
                

                    <%=orders.address[0].locality%><br>
                    <%=orders.address[0].city%>
                    <%=orders.address[0].region%><br>
                    <%=orders.address[0].country%>
                    <%=orders.address[0].post_code%>
                </td>
                <td>
                  
                </td>
                <td>
                  <%=orders.discount%>
                </td>
                <td> </td>
                <td>
                  <%=orders.total%>
                </td>
                <td> </td>
                <td>
                  <%=orders.payamentType%><br>
                  <%=orders.paymentStatus%>
                </td>
                
                <td>
                  <%=orders.deliveryStatus%>
                </td>
                  </tr>
                <%}%>
                </table>
                </div>
                
                
              </div>  
            </div>
          </div>
          <!-- END CONTENT -->
        </div>
        <!-- END SIDEBAR & CONTENT -->
      </div>
    </div>
    <%-include("./partials/footer")%>
    <!-- BEGIN BRANDS -->
    
    <!-- END BRANDS -->

